#!ipxe

echo Landed to start page

imgfetch http://mirror.centos.org/centos/7/os/x86_64/isolinux/initrd.img
imgfetch http://mirror.centos.org/centos/7/os/x86_64/isolinux/vmlinuz

imgverify vmlinuz vmlinuz.sig
imgverify initrd.img initrd.img.sig

initrd initrd.img
kernel vmlinuz text utf8 inst.ks=https://raw.githubusercontent.com/ouspg/secudep/master/boot/centos7.ks

boot
